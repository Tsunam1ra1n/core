{
  "name": "@phantomchain/core-snapshots",
  "description": "Provides live local streamed snapshots functionality for PHANTOM Core",
  "version": "0.1.1",
  "contributors": [
    "Kristjan Košič <chris@ark.io>"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "test": "cross-env PHANTOM_ENV=test jest --runInBand --detectOpenHandles",
    "test:coverage": "cross-env PHANTOM_ENV=test jest --coverage --runInBand --detectOpenHandles",
    "test:debug": "cross-env PHANTOM_ENV=test node --inspect-brk ../../node_modules/.bin/jest --runInBand --watch",
    "test:watch": "cross-env PHANTOM_ENV=test jest --runInBand --watch",
    "test:watch:all": "cross-env PHANTOM_ENV=test jest --runInBand --watchAll",
    "flow:coverage": "flow-coverage-report --config ../../.flowcoverage.json",
    "flow:check": "flow check ./lib",
    "flow:deps": "flow-typed install",
    "lint": "eslint ./ --fix",
    "depcheck": "depcheck ./ --ignores=flow-bin"
  },
  "dependencies": {
    "@phantomchain/core-container": "^0.1.4",
    "@phantomchain/core-database-postgres": "^0.1.2",
    "@phantomchain/crypto": "^0.2.6",
    "JSONStream": "^1.3.5",
    "bluebird": "^3.5.3",
    "create-hash": "^1.2.0",
    "fs-extra": "^7.0.1",
    "lodash": "^4.17.11",
    "msgpack-lite": "^0.1.26",
    "pg-promise": "^8.5.2",
    "pg-query-stream": "^1.1.2",
    "xcase": "^2.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10.x"
  },
  "devDependencies": {
    "flow-bin": "^0.86.0"
  }
}
